# Example environment variables for Poseidon application
# Copy this file to .env (which is ignored by git) and fill in your actual values

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=poseidon
DB_USERNAME=your_username
DB_PASSWORD=your_password

# Test Database Configuration
TEST_DB_HOST=localhost
TEST_DB_PORT=5432
TEST_DB_NAME=poseidon_test
TEST_DB_USERNAME=your_test_username
TEST_DB_PASSWORD=your_test_password

# Security Configuration
JWT_SECRET=your_jwt_secret_key_here
ENCRYPTION_PASSWORD=your_encryption_password_here

# Application Configuration
APP_ENV=dev
SERVER_PORT=8080

# External APIs (if any)
EXTERNAL_API_KEY=your_api_key_here
EXTERNAL_API_URL=https://api.example.com
